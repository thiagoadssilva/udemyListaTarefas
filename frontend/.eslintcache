refa] = useState('');\n    const [formValidado, setFormValidado] = useState(false);\n    const [exibirModal, setExibirModal] = useState(false);\n\n    function cadastrar(event) {\n        event.preventDefault();\n        setFormValidado(true);\n        if(event.currentTarget.checkValidity() === true){\n            const tarefasDb = localStorage['tarefas'];\n            const tarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\n\n            tarefas.push(new Tarefa(new Date().getTime(), tarefa, false));\n            localStorage['tarefas'] = JSON.stringify(tarefas);\n            setExibirModal(true);\n        }\n    }\n\n    function handleTxtTarefa(event) {\n        setTarefa(event.target.value);\n    }\n\n    function handleFecharModal(){\n        navigate('/');\n    }\n\n    return (\n        <Container>\n            <h3 className=\"text-center\">Cadastrar</h3>\n            <Jumbotron>\n                <Form validated={formValidado} noValidate onSubmit={cadastrar}>\n                    <Form.Group>\n                        <Form.Label>Tarefa:</Form.Label>\n                        <Form.Control\n                            type=\"text\"\n                            placeholder=\"Informe a Tarefa\"\n                            minLength=\"5\"\n                            maxLength=\"100\"\n                            required\n                            value={tarefa}\n                            onChange={handleTxtTarefa}\n                            data-testid=\"txt-tarefa\"\n                        >\n                            {/* <Form.Control.Feedback type=\"invalid\">\n                                A tarefa deve conter ao menos 5 caracteres.\n                            </Form.Control.Feedback> */}\n\n                        </Form.Control>\n                    </Form.Group>\n                    <Form.Group>\n                        <Button\n                            variant=\"success\"\n                            type=\"submit\"\n                            className=\" btn btn-success btn-sm\"\n                            data-testid=\"btn-cadastrar\"\n                        >\n                            Cadastrar\n                        </Button>\n                        &nbsp;\n                        <A href=\"/\" className=\"btn btn-light\">Voltar</A>\n                    </Form.Group>\n                </Form>\n                <Modal show={exibirModal} onHide={handleFecharModal} data-testid=\"modal\">\n                    <Modal.Header closeButton>\n                        <Modal.Title>Sucesso!!</Modal.Title>\n                    </Modal.Header>\n                    <Modal.Body>\n                        Tarefa Cadastrada com Sucesso!!\n                    </Modal.Body>\n                    <Modal.Footer>\n                        <Button variant=\"success\" onClick={handleFecharModal}>\n                            Continuar\n                        </Button>\n                    </Modal.Footer>\n                </Modal>\n            </Jumbotron>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/AtualizarTarefa/index.js",["84"],"/var/www/udemyListaTarefas/frontend/src/components/ListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/CadastrarTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/models/tarefa.model.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/index.js",["85"],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/index.js",["86"],"/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/index.js",["87"],"/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/Paginacao/index.js",["88"],"/var/www/udemyListaTarefas/frontend/src/components/P
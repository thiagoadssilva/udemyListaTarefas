\n\n\nimport {\n    Container\n} from './styled';\n\nexport default () => {\n\n    const ITEMS_POR_PAG = 5;\n    const API_URL_LISTAR_TAREFAS = \"http://localhost:3002/gerenciador-tarefas\";\n\n    const [tarefas, setTarefas] = useState([]);\n    const [carregarTarefas, setCarregarTarefas] = useState(true);\n    const [totalItems, setTotalItems] = useState(0);\n    const [paginaAtual, setPaginaAtual] = useState(1);\n    const [ordenarAsc, setOrdenarAsc] = useState(false);\n    const [ordenarDesc, setOrdenarDesc] = useState(false);\n    const [filtroTarefa, setFiltroTarefa] = useState('');\n\n    useEffect(() => {\n        async function obterTarefas() {\n\n            //- INICIO Criando a ordenação\n            let ordem = '';\n\n            if (ordenarAsc) {\n                ordem = 'ASC';\n            } else if (ordenarDesc) {\n                ordem = 'DESC';\n            }\n            //- FIM Criando a ordenação\n\n            try {\n                const params = `?pag=${paginaAtual}&ordem=${ordem}&filtro-tarefa=${filtroTarefa}&itens-por-pagina=${ITEMS_POR_PAG}`;\n                let { data } = await axios.get(API_URL_LISTAR_TAREFAS + params);\n                setTotalItems(data.totalItens);\n                setTarefas(data.tarefas);\n            } catch (error) {\n                setTarefas([]);\n            }\n            //- INCIO Código que representa a criação do localstorage, foi comentado para entrada da api\n            // const tarefasDb = localStorage['tarefas'];\n            // let listaTarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\n            // //- Filtrar a tarefa\n            // listaTarefas = listaTarefas.filter(t => t.nome.toLowerCase().indexOf(filtroTarefa.toLocaleLowerCase()) === 0);\n            // //- Ordenação\n            // if (ordenarAsc) {\n            //     listaTarefas.sort((t1, t2) => (t1.nome.toLowerCase() > t2.nome.toLowerCase()) ? 1 : -1);\n            // } else if (ordenarDesc) {\n            //     listaTarefas.sort((t1, t2) => (t1.nome.toLowerCase() < t2.nome.toLowerCase()) ? 1 : -1);\n            // }\n            // //- Paginação\n            // setTotalItems(listaTarefas.length);\n            // setTarefas(listaTarefas.splice((paginaAtual - 1) * ITEMS_POR_PAG, ITEMS_POR_PAG));\n            //- FIM Código que representa a criação do localstorage, foi comentado para entrada da api\n        }\n\n        if (carregarTarefas) {\n            obterTarefas();\n            setCarregarTarefas(false);\n        }\n\n    }, [carregarTarefas, paginaAtual, ordenarDesc, ordenarAsc, filtroTarefa]);\n\n    function handleMudarPagina(pagina) {\n        setPaginaAtual(pagina);\n        setCarregarTarefas(true);\n    }\n\n    function handleOrdenar(event) {\n        event.preventDefault();\n\n        if (!ordenarAsc && !ordenarDesc) {\n            setOrdenarAsc(true);\n            setOrdenarDesc(false);\n        } else if (ordenarAsc) {\n            setOrdenarAsc(false);\n            setOrdenarDesc(true);\n        } else {\n            setOrdenarDesc(false);\n            setOrdenarAsc(false);\n        }\n        setCarregarTarefas(true);\n    }\n\n    function handleFiltrar(event) {\n        setFiltroTarefa(event.target.value);\n        setCarregarTarefas(true);\n    }\n\n    return (\n        <Container>\n            <div class=\"table-responsive\" className=\"text-center\">\n                <h3 >Lista a Fazer</h3>\n                <Table\n                    striped\n                    bordered\n                    hover\n                    responsive\n                    data-testid=\"tabela\"\n                >\n                    <thead>\n                        <tr>\n                            <th>\n                                <a className=\"teste\" href=\"/\" onClick={handleOrdenar}>\n                                    Tarefa\n                                    &nbsp;\n                                    <Ordenacao ordenarAsc={ordenarAsc} ordenarDesc={ordenarDesc} />\n                                </a>\n                            </th>\n                            <th className=\"text-center\">\n                                <A href=\"/cadastrar\" className=\"btn btn-success btn-sm\" data-testid=\"btn-nova-tarefa\">\n                                    <FontAwesomeIcon icon={faPlus} />\n                                    &nbsp;\n                                    Nova Tarefa\n                                </A>\n                            </th>\n                        </tr>\n                        <tr>\n                            <th>\n                                <Form.Control placeholder=\"Informa uma tarefa\" className=\"filtroTarefa\" type=\"text\" value={filtroTarefa} onChange={handleFiltrar} data-testid=\"txt-tarefa\" />\n                            </th>\n                            <th>&nbsp;</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <ItensListaTarefas\n                            tarefas={tarefas}\n                            recarregarTarefas={setCarregarTarefas}\n                        />\n                    </tbody>\n                </Table>\n                <Paginacao totalItems={totalItems} itemsPorPagina={ITEMS_POR_PAG} paginaAtual={paginaAtual} mudarPagina={handleMudarPagina} />\n            </div>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/AtualizarTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/CadastrarTarefa/index.js",["89"],"import React, { useState } from 'react';\nimport { Button, Form, Jumbotron, Modal } from 'react-bootstrap';\nimport { navigate, A } from 'hookrouter';\nimport Tarefa from '../../models/tarefa.model';\nimport axios from 'axios';\n\nimport {\n    Container\n} from './styled';\n\nexport default () => {\n    const API_URL_CADASTRAR_TAREFA = \"http://localhost:3002/gerenciador-tarefas\";\n\n    const [tarefa, setTarefa] = useState('');\n    const [formValidado, setFormValidado] = useState(false);\n    const [exibirModal, setExibirModal] = useState(false);\n    const [exibirModalErro, setExibirModalErro] = useState(false);\n\n    async function cadastrar(event) {\n        event.preventDefault();\n        setFormValidado(true);\n        if (event.currentTarget.checkValidity() === true) {\n            try {\n                const novaTarefa = new Tarefa(null, tarefa, false);\n                await axios.post(API_URL_CADASTRAR_TAREFA, novaTarefa);\n                setExibirModal(true);\n            } catch (error) {\n                setExibirModalErro(true);\n            }\n            //- INICIO Parte referente ao uso do localStorage\n            // const tarefasDb = localStorage['tarefas'];\n            // const tarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\n\n            // tarefas.push(new Tarefa(new Date().getTime(), tarefa, false));\n            // localStorage['tarefas'] = JSON.stringify(tarefas);\n            // setExibirModal(true);\n            //- FIM Parte referente ao uso do localStorage\n        }\n    }\n\n    function handleTxtTarefa(event) {\n        setTarefa(event.target.value);\n    }\n\n    function handleFecharModal() {\n        navigate('/');\n    }\n\n    function handleFecharModalErro(){\n        setExibirModalErro(false);\n    }\n\n    return (\n        <Container>\n            <h3 className=\"text-center\">Cadastrar</h3>\n            <Jumbotron>\n                <Form validated={formValidado} noValidate onSubmit={cadastrar}>\n                    <Form.Group>\n                        <Form.Label>Tarefa:</Form.Label>\n                        <Form.Control\n                            type=\"text\"\n                            placeholder=\"Informe a Tarefa\"\n                            minLength=\"5\"\n                            maxLength=\"100\"\n                            required\n                            value={tarefa}\n                            onChange={handleTxtTarefa}\n                            data-testid=\"txt-tarefa\"\n                        >\n                            {/* <Form.Control.Feedback type=\"invalid\">\n                                A tarefa deve conter ao menos 5 caracteres.\n                            </Form.Control.Feedback> */}\n\n                        </Form.Control>\n                    </Form.Group>\n                    <Form.Group>\n                        <Button\n                            variant=\"success\"\n                            type=\"submit\"\n                            className=\" btn btn-success btn-sm\"\n                            data-testid=\"btn-cadastrar\"\n                        >\n                            Cadastrar\n                        </Button>\n                        &nbsp;\n                        <A href=\"/\" className=\"btn btn-light\">Voltar</A>\n                    </Form.Group>\n                </Form>\n                <Modal show={exibirModal} onHide={handleFecharModal} data-testid=\"modal\">\n                    <Modal.Header closeButton>\n                        <Modal.Title>Sucesso!!</Modal.Title>\n                    </Modal.Header>\n                    <Modal.Body>\n                        Tarefa Cadastrada com Sucesso!!\n                    </Modal.Body>\n                    <Modal.Footer>\n                        <Button variant=\"success\" onClick={handleFecharModal}>\n                            Continuar\n                        </Button>\n                    </Modal.Footer>\n                </Modal>\n\n                <Modal show={exibirModalErro} onHide={handleFecharModalErro}>\n                    <Modal.Header closeButton>\n                        <Modal.Title>Erro</Modal.Title>\n                    </Modal.Header>\n                    <Modal.Body>\n                        Erro ao adicionar a tarefa, por favor tente novamente!\n                    </Modal.Body>\n                    <Modal.Footer>\n                        <Button variant=\"warning\" onClick={handleFecharModalErro}>\n                            Fechar\n                        </Button>\n                    </Modal.Footer>\n                </Modal>\n            </Jumbotron>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/AtualizarTarefa/index.js",["90"],"/var/www/udemyListaTarefas/frontend/src/components/ListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/CadastrarTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/models/tarefa.model.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/index.js",["91"],"import React from 'react';\n//import PropTypes from 'prop-types';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faEdit } from '@fortawesome/free-solid-svg-icons';\nimport { A } from 'hookrouter';\nimport ConcluirTarefa from '../ConcluirTarefa';\nimport RemoverTarefa from '../RemoverTarefa';\n\n// import {\n//     Container\n// } from './styled';\n\n\n\nexport default (props) => {\n\n    function marcarConcluida(tarefa) {\n        return tarefa.concluida ? 'line-through' : 'none';\n    }\n\n    return (\n        <>\n            {props.tarefas.map((item) => \n                <tr key={item.id} data-testid=\"taf\">\n                    <td width=\"75%\" data-testid=\"nome-tarefa\" style={{ textDecoration: marcarConcluida(item) }}>\n                        {item.nome}\n                    </td>\n                    <td style={{display: 'flex', justifyContent: 'center', alignItems: 'center'}}>\n                        <ConcluirTarefa tarf={item} recarregarTarefas={props.recarregarTarefas} ocultarIcone={item.concluida ? 'hidden' : null}/>\n                        {item.concluida === false &&\n                            <A href={\"/atualizar/\" + item.id} className=\"btn btn-warning btn-sm\">\n                                <FontAwesomeIcon icon={faEdit} />\n                            </A>\n                        }\n                        <RemoverTarefa  tarf={item} recarregarTarefas={props.recarregarTarefas}/>\n                    </td>\n                </tr>\n            )}\n        </>\n    );\n}",["92","93"],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/index.js",["94","95"],"/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/index.js",["96","97"],"/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/Paginacao/index.js",["98"],"import React from 'react';\nimport Pagination from 'react-bootstrap/Pagination';\n\nimport {\n    Container\n} from './styled';\n\nexport default ({ mudarPagina, paginaAtual, totalItems, itemsPorPagina }) => {\n\n    function gerarPrimeiroItem() {\n        return (\n            <Pagination.First key=\"pagfirst\" onClick={() => mudarPagina(1)} disabled={paginaAtual === 1} />\n        );\n    }\n\n    function gerarItemAnterior() {\n        return (\n            <Pagination.Prev key=\"pagPrev\" onClick={() => mudarPagina(paginaAtual - 1)} disabled={paginaAtual === 1} />\n        );\n    }\n\n    function gerarItemNumerico(pagina) {\n        return (\n            <Pagination.Item key={pagina} active={pagina === paginaAtual} onClick={() => mudarPagina(pagina)}>\n                {pagina}\n            </Pagination.Item>\n        );\n    }\n\n    function gerarProximoItem(numPaginas) {\n        return (\n            <Pagination.Next key=\"pagNext\" onClick={() => mudarPagina(paginaAtual + 1)} disabled={paginaAtual === numPaginas} />\n        );\n    }\n\n    function gerarUltimoItem(numPaginas) {\n        return (\n            <Pagination.Last key=\"pagLast\" onClick={() => mudarPagina(numPaginas)} disabled={paginaAtual === numPaginas}/>\n        );\n    }\n\n    function obterPaginacao() {\n        const numPaginas = Math.ceil(totalItems / itemsPorPagina); \n        let items = [];\n\n        items.push(gerarPrimeiroItem());\n        items.push(gerarItemAnterior());\n\n        for (let pagina = 1; pagina <= numPaginas; pagina++) {\n            items.push(gerarItemNumerico(pagina));\n        }\n\n        items.push(gerarProximoItem(numPaginas));\n        items.push(gerarUltimoItem(numPaginas));\n\n        return items;\n    }\n\n    return (\n        <Container>\n            <Pagination data-testid=\"paginacao\">\n                {obterPaginacao()}\n            </Pagination>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/Paginacao/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/Ordenacao/index.js",["99","100"],"import React, {useEffect} from 'react';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faSort, faSortUp, faSortDown } from '@fortawesome/free-solid-svg-icons';\n\nimport {\n    Container,\n    FaSortContainer,\n    FaSortUpContainer,\n    FaSortDownContainer\n} from './styled';\n\nexport default (props) => {\n    \n    // function handleAscDesc() {\n    //     return(props.ordenarAsc || props.ordenarDesc) ? 'hidden' : '';\n    // }\n\n    // function handleAsc(){\n    //     return props.ordenarAsc ? '' : 'hidden';\n    // }\n\n    // function handleDesc(){\n    //     return props.ordenarDesc ? '' : 'hidden';\n    // }\n\n    return (\n        <Container>\n\n            <FaSortContainer faSorteAsc={props.ordenarAsc} faSorteDesc={props.ordenarDesc} data-testid=\"faSort\">\n                <FontAwesomeIcon icon={faSort}/>\n            </FaSortContainer>\n\n            <FaSortUpContainer faSorteUpAsc={props.ordenarAsc} data-testid=\"faSortUp\">\n                <FontAwesomeIcon icon={faSortUp}/>\n            </FaSortUpContainer>\n\n            <FaSortDownContainer faSorteDownDesc={props.ordenarDesc} data-testid=\"faSortDown\">\n                <Font
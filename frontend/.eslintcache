refa.model';\n\nimport {\n    Container\n} from './styled';\n\nexport default () => {\n    const [tarefa, setTarefa] = useState('');\n    const [formValidado, setFormValidado] = useState(false);\n    const [exibirModal, setExibirModal] = useState(false);\n\n    function cadastrar(event) {\n        event.preventDefault();\n        setFormValidado(true);\n        if(event.currentTarget.checkValidity() === true){\n            const tarefasDb = localStorage['tarefas'];\n            const tarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\n\n            tarefas.push(new Tarefa(new Date().getTime(), tarefa, false));\n            localStorage['tarefas'] = JSON.stringify(tarefas);\n            setExibirModal(true);\n        }\n    }\n\n    function handleTxtTarefa(event) {\n        setTarefa(event.target.value);\n    }\n\n    function handleFecharModal(){\n        navigate('/');\n    }\n\n    return (\n        <Container>\n            <h3 className=\"text-center\">Cadastrar</h3>\n            <Jumbotron>\n                <Form validated={formValidado} noValidate onSubmit={cadastrar}>\n                    <Form.Group>\n                        <Form.Label>Tarefa:</Form.Label>\n                        <Form.Control\n                            type=\"text\"\n                            placeholder=\"Informe a Tarefa\"\n                            minLength=\"5\"\n                            maxLength=\"100\"\n                            required\n                            value={tarefa}\n                            onChange={handleTxtTarefa}\n                            data-testid=\"txt-tarefa\"\n                        >\n                            {/* <Form.Control.Feedback type=\"invalid\">\n                                A tarefa deve conter ao menos 5 caracteres.\n                            </Form.Control.Feedback> */}\n\n                        </Form.Control>\n                    </Form.Group>\n                    <Form.Group>\n                        <Button\n                            variant=\"success\"\n                            type=\"submit\"\n                            className=\" btn btn-success btn-sm\"\n                            data-testid=\"btn-cadastrar\"\n                        >\n                            Cadastrar\n                        </Button>\n                        &nbsp;\n                        <A href=\"/\" className=\"btn btn-light\">Voltar</A>\n                    </Form.Group>\n                </Form>\n                <Modal show={exibirModal} onHide={handleFecharModal} data-testid=\"modal\">\n                    <Modal.Header closeButton>\n                        <Modal.Title>Sucesso!!</Modal.Title>\n                    </Modal.Header>\n                    <Modal.Body>\n                        Tarefa Cadastrada com Sucesso!!\n                    </Modal.Body>\n                    <Modal.Footer>\n                        <Button variant=\"success\" onClick={handleFecharModal}>\n                            Continuar\n                        </Button>\n                    </Modal.Footer>\n                </Modal>\n            </Jumbotron>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/AtualizarTarefa/index.js",["88"],"import React from 'react';\n\nimport {\n    Container\n} from './styled';\n\nexport default () => {\n    return (\n        <Container>\n            <h1>Atualiza Tarefa</h1>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/ListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/CadastrarTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/models/tarefa.model.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/index.js",["89"],"/var/www/udemyListaTarefas/frontend/src/components/ItensListaTarefas/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/index.js",["90"],"import React, {useState} from 'react';\nimport {Modal, Button} from 'react-bootstrap';\nimport {FontAwesomeIcon} from '@fortawesome/react-fontawesome';\nimport {faClipboardCheck} from '@fortawesome/free-solid-svg-icons';\n\nimport {\n    Container\n} from './styled';\n\nexport default ({tarf, recarregarTarefas, ocultarIcone}) => {\n    const [exibirMOdal, setExibirModal] = useState(false);\n\n    function handleAbrirModal(event){\n        event.preventDefault();\n        setExibirModal(true);\n    }\n\n    function handleFecharModal(){\n        setExibirModal(false);\n    }\n\n    function handleConcluirTarefa(event){\n        event.preventDefault();\n        const tarefaDb = localStorage['tarefas'];\n        let tarefas = tarefaDb ? JSON.parse(tarefaDb) : [];\n        tarefas = tarefas.map(tarefa => {\n            if(tarefa.id === tarf.id){\n                tarefa.concluida = true;\n            }\n            return tarefa;\n        });\n        localStorage['tarefas'] = JSON.stringify(tarefas);\n        setExibirModal(false);\n        recarregarTarefas(true);\n    }\n    return(\n        <Container className={tarf.concluida} >\n            <Button className=\"btn-sm\" onClick={handleAbrirModal} data-testid=\"btn-abrir-modal\"> \n                <FontAwesomeIcon icon={faClipboardCheck} />\n            </Button>\n\n            <Modal show={exibirMOdal} onHide={handleFecharModal} data-testid=\"modal\">\n                <Modal.Header closeButton>\n                    <Modal.Title>Concluir Tarefa</Modal.Title>\n                </Modal.Header>\n\n                <Modal.Body>\n                    Deseja Realmente concluir a seguinte tarefa?\n                    <br />\n                    <strong>{tarf.nome}</strong>\n                </Modal.Body>\n\n                <Modal.Footer>\n                    <Button variant=\"primary\" onClick={handleConcluirTarefa} data-testid=\"btn-concluir\">\n                        SIM\n                    </Button>\n                    <Button variant=\"light\" onClick={handleFecharModal} data-testid=\"btn-fechar-modal\">\n                        NÃO\n                    </Button>\n                </Modal.Footer>\n            </Modal>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/ConcluirTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/index.js",["91"],"import React, { useState } from 'react';\nimport { Modal, Button } from 'react-bootstrap';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faTrashAlt } from '@fortawesome/free-solid-svg-icons';\n\nimport {\n    Container\n} from './styled';\n\nexport default ({ tarf, recarregarTarefas }) => {\n    const [exibirModal, setExibirModal] = useState(false);\n    let texto = \"Deseja Realmente excluir a tarefa?\";\n\n    function handleAbrirModal(event) {\n        event.preventDefault();\n        setExibirModal(true);\n    }\n\n    function handleFecharModal() {\n        setExibirModal(false);\n    }\n\n    function handleRemoverTarefa(event){\n        event.preventDefault();\n        const tarefasDb = localStorage['tarefas'];\n        let tarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\n        tarefas = tarefas.filter(tarefa => tarefa.id !== tarf.id);\n        localStorage['tarefas'] = JSON.stringify(tarefas);\n        setExibirModal(false);\n        recarregarTarefas(true);\n    }\n\n    return (\n        <Container>\n            <Button\n                variant=\"danger\"\n                className=\"btn-sm\"\n                onClick={handleAbrirModal}\n                data-testid=\"btn-abrir-modal\"\n            >\n                <FontAwesomeIcon icon={faTrashAlt} />\n            </Button>\n\n            <Modal show={exibirModal} onHide={handleFecharModal} data-testid=\"modal\">\n                <Modal.Header closeButton>\n                    <Modal.Title>Remover Tarefa</Modal.Title>\n                </Modal.Header>\n\n                <Modal.Body>\n                    {texto}\n                    <br />\n                    <strong>{tarf.nome}</strong>\n                </Modal.Body>\n\n                <Modal.Footer>\n                    <Button variant=\"primary\" onClick={handleRemoverTarefa} data-testid=\"btn-remover\">\n                        SIM\n                    </Button>\n                    <Button variant=\"light\" onClick={handleFecharModal}>\n                        NÃO\n                    </Button>\n                </Modal.Footer>\n\n            </Modal>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/RemoverTarefa/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/Paginacao/index.js",["92"],"import React from 'react';\nimport Pagination from 'react-bootstrap/Pagination';\n\nimport {\n    Container\n} from './styled';\n\nexport default ({ mudarPagina, paginaAtual, totalItems, itemsPorPagina }) => {\n\n    function gerarPrimeiroItem() {\n        return (\n            <Pagination.First key=\"pagfirst\" onClick={() => mudarPagina(1)} disabled={paginaAtual === 1} />\n        );\n    }\n\n    function gerarItemAnterior() {\n        return (\n            <Pagination.Prev key=\"pagPrev\" onClick={() => mudarPagina(paginaAtual - 1)} disabled={paginaAtual === 1} />\n        );\n    }\n\n    function gerarItemNumerico(pagina) {\n        return (\n            <Pagination.Item key={pagina} active={pagina === paginaAtual} onClick={() => mudarPagina(pagina)}>\n                {pagina}\n            </Pagination.Item>\n        );\n    }\n\n    function gerarProximoItem(numPaginas) {\n        return (\n            <Pagination.Next key=\"pagNext\" onClick={() => mudarPagina(paginaAtual + 1)} disabled={paginaAtual === numPaginas} />\n        );\n    }\n\n    function gerarUltimoItem(numPaginas) {\n        return (\n            <Pagination.Last key=\"pagLast\" onClick={() => mudarPagina(numPaginas)} disabled={paginaAtual === numPaginas}/>\n        );\n    }\n\n    function obterPaginacao() {\n        const numPaginas = Math.ceil(totalItems / itemsPorPagina); \n        let items = [];\n\n        items.push(gerarPrimeiroItem());\n        items.push(gerarItemAnterior());\n\n        for (let pagina = 1; pagina <= numPaginas; pagina++) {\n            items.push(gerarItemNumerico(pagina));\n        }\n\n        items.push(gerarProximoItem(numPaginas));\n        items.push(gerarUltimoItem(numPaginas));\n\n        return items;\n    }\n\n    return (\n        <Container>\n            <Pagination data-testid=\"paginacao\">\n                {obterPaginacao()}\n            </Pagination>\n        </Container>\n    );\n}","/var/www/udemyListaTarefas/frontend/src/components/Paginacao/styled.js",[],"/var/www/udemyListaTarefas/frontend/src/components/Ordenacao/index.js",["93","94"],"/var/www/udemyListaTarefas/frontend/src/components/Ordenacao/styled.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":15,"column":1,"nodeType":"101","endLine":109,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":10,"column":1,"nodeType":"101","endLine":88,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":7,"column":1,"nodeType":"101","endLine":13,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":15,"column":1,"nodeType":"101","endLine":41,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":10,"column":1,"nodeType":"101","endLine":64,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":10,"column":1,"nodeType":"101","endLine":67,"endColumn":2},{"ruleId":"99","severity":1,"message":"100","line":8,"column":1,"nodeType":"101","endLine":66,"endColumn":2},{"ruleId":"102","severity":1,"message":"103","line":1,"column":16,"nodeType":"104","messageId":"105","endLine":1,"endColumn":25},{"ruleId":"99","severity":1,"message":"100","line":12,"column":1,"nodeType":"101","endLine":44,"endColumn":2},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"import/no-anonymous-